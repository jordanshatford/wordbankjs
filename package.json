{
  "name": "wordlist-js",
  "version": "1.5.1",
  "description": "A package to provide common English words from various nationalities.",
  "main": "./dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "npm run clean && npm run generate && tsc && npm run uglify:dist",
    "uglify:dist": "uglifyjs-folder dist -e -x .js -o dist --log-level error",
    "lint": "eslint ./src/",
    "lint:fix": "eslint ./src/ --fix",
    "typecheck": "tsc --noEmit",
    "generate": "ts-node src/cli.ts -s src/assets/scowl-2020.12.07/final/ -o src/generated/",
    "clean": "rm -rf dist/ && rm -rf src/generated/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jordanshatford/wordlist-js.git"
  },
  "license": "MIT",
  "author": {
    "name": "Jordan Shatford",
    "url": "https://github.com/jordanshatford"
  },
  "keywords": [
    "typescript",
    "words",
    "english",
    "american",
    "australian",
    "british",
    "britishZ",
    "canadian",
    "words",
    "wordslist",
    "wordlist",
    "scrabble",
    "wordle"
  ],
  "bugs": {
    "url": "https://github.com/jordanshatford/wordlist-js/issues"
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.3",
    "@semantic-release/npm": "^11.0.1",
    "@types/bad-words": "^3.0.3",
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.9.1",
    "@types/yargs": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "bad-words": "^3.0.4",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "mustache": "^4.2.0",
    "prettier": "^3.1.0",
    "semantic-release": "^22.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "uglifyjs-folder": "^3.3.0",
    "yargs": "^17.7.2"
  },
  "packageManager": "pnpm@8.10.5+sha256.a4bd9bb7b48214bbfcd95f264bd75bb70d100e5d4b58808f5cd6ab40c6ac21c5",
  "engines": {
    "pnpm": ">=8",
    "node": ">=18"
  }
}
