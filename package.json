{
  "name": "wordlist-js",
  "version": "1.8.0",
  "description": "A package to provide common English words from various nationalities.",
  "main": "./dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "changeset": "changeset",
    "commitlint": "commitlint --edit",
    "build": "pnpm clean && pnpm generate && tsc && pnpm uglify:dist",
    "uglify:dist": "uglifyjs-folder dist -e -x .js -o dist --log-level error",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "generate": "ts-node scripts/generate.ts",
    "clean": "ts-node scripts/clean.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jordanshatford/wordlist-js.git"
  },
  "license": "MIT",
  "author": {
    "name": "Jordan Shatford",
    "url": "https://github.com/jordanshatford"
  },
  "keywords": [
    "typescript",
    "words",
    "english",
    "american",
    "australian",
    "british",
    "britishZ",
    "canadian",
    "words",
    "wordslist",
    "wordlist",
    "scrabble",
    "wordle"
  ],
  "bugs": {
    "url": "https://github.com/jordanshatford/wordlist-js/issues"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@commitlint/types": "^18.4.3",
    "@svitejs/changesets-changelog-github-compact": "^1.1.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "uglifyjs-folder": "^3.3.0"
  },
  "packageManager": "pnpm@8.10.5+sha256.a4bd9bb7b48214bbfcd95f264bd75bb70d100e5d4b58808f5cd6ab40c6ac21c5",
  "engines": {
    "pnpm": ">=8",
    "node": ">=18"
  }
}
